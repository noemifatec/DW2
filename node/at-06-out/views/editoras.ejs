<%- include('partials/header') %>

<h1>Editoras</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Editora</th>
    </tr>
  </thead>
  <tbody>
    <% listaEditoras.forEach((editora, index) => { %>
      <tr>
        <td><%= index + 1 %></td>
        <td><%= editora %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

<%- include('partials/footer') %>

